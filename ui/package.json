{
  "name": "replication-ui",
  "author": "Connexta",
  "description": "Replication UI for configuring and managing replication",
  "version": "0.0.1",
  "main": "index.js",
  "license": "LGPL-3.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "http://github.com/connexta/replication.git"
  },
  "scripts": {
    "start": "webpack-dev-server --mode development --progress --open",
    "lint": "eslint src",
    "pretest": "yarn lint",
    "test": "jest",
    "format": "prettier \"**/*+(.tsx|.js|.less|.json|.css)\"",
    "fmt": "yarn format --write",
    "prebuild": "yarn audit",
    "build": "webpack --mode production"
  },
  "dependencies": {
    "@babel/polyfill": "7.2.5",
    "@material-ui/core": "3.9.2",
    "@material-ui/icons": "3.0.2",
    "apollo-cache-inmemory": "1.5.1",
    "apollo-client": "2.5.1",
    "apollo-link-http": "1.5.14",
    "graphql": "14.0.2",
    "graphql-tag": "2.10.0",
    "immutable": "3.8.2",
    "moment": "2.24.0",
    "notistack": "0.5.0",
    "prop-types": "15.6.2",
    "react": "16.8.3",
    "react-apollo": "2.3.3",
    "react-dom": "16.8.3",
    "react-router-dom": "4.3.1",
    "unfetch": "4.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.2.0",
    "@babel/plugin-proposal-class-properties": "7.2.3",
    "@babel/preset-env": "7.2.0",
    "@babel/preset-react": "7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.1.0",
    "babel-loader": "8.0.4",
    "css-loader": "2.1.0",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "eslint": "5.12.1",
    "eslint-config-prettier": "4.0.0",
    "eslint-plugin-html": "5.0.0",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.1.0",
    "prettier": "1.15.3",
    "react-hot-loader": "4.6.3",
    "react-test-renderer": "16.7.0",
    "style-loader": "0.23.1",
    "webpack": "4.28.2",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.14",
    "webpack-merge": "4.1.5"
  },
  "jest": {
    "setupFiles": [
      "./jestsetup.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 7,
        "branches": 6,
        "lines": 7,
        "functions": 2
      }
    },
    "collectCoverage": true,
    "coverageDirectory": "./target/coverage",
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/gql/*.js",
      "!webpack.config.js",
      "!**/node_modules/**",
      "!**/node/**",
      "!**/target/**"
    ]
  }
}
